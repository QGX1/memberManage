@import "./../components/diary/diary-list.wxss";
@import "./../components/diary/diary-head.wxss";

.container{
  background-color:#eee;
  overflow: hidden;
  width: 100vw;
  /*height: 100vh;*/
}
.diary-head{
  position:fixed;
  width:100%;
  top:0;
  z-index:1024;
  height: 172rpx;
  transition-duration:0.5s;
}
.diary-head.show{
  transform:translateY(0%);
  
}
.diary-head.hidden{
  transform:translateY(-100%);
}
