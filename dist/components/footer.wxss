.footer_bar {
  position: fixed;
  bottom: 0%;
  display: flex;
  z-index: 999;
  left: 0;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-top: 1rpx solid #ccc;
  background-color: #fff;
}
.footer_bar navigator {
  display: inline-block;
  flex: 1;
  color: #bbb;
}
.footer_bar .active {
  font-weight: 700;
  color: #42A4FF;
}
